{
  "name": "user-management",
  "version": "0.0.1",
  "private": true,
  "description": "Simple user management written in Angular2 and Typescript. A playground for trying all development and release scenarios.",
  "keywords": [
    "angular2",
    "rxjs",
    "webpack",
    "hmr",
    "typescript"
  ],
  "author": "mcibique@hotmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mcibique/angular2-playground/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mcibique/angular2-playground.git"
  },
  "scripts": {
    "build": "webpack --progress --profile --colors --display-error-details --display-cached",
    "serve": "webpack-dev-server --inline --progress --profile --colors --hot --watch --display-error-details --display-cached --content-base src/",
    "clean": "rimraf -- dist",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "tslint": "tslint -s ./node_modules/custom-tslint-formatters/formatters -t grouped ./src/**/*.ts",
    "sass-lint": "sass-lint -v -q",
    "htmlhint": "htmlhint"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "core-js": "2.4.1",
    "normalize-scss": "5.0.3",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "@types/core-js": "0.9.28",
    "@types/jasmine": "2.2.29",
    "@types/node": "4.0.29",
    "angular2-hmr": "0.8.1",
    "autoprefixer-loader": "3.2.0",
    "awesome-typescript-loader": "2.1.0",
    "css-loader": "0.23.1",
    "custom-tslint-formatters": "1.1.0",
    "html-webpack-plugin": "2.22.0",
    "htmlhint-loader": "0.1.0",
    "node-sass": "3.8.0",
    "raw-loader": "0.5.1",
    "sass-lint": "1.8.2",
    "sass-loader": "4.0.0",
    "sasslint-webpack-plugin": "1.0.3",
    "style-loader": "0.13.1",
    "tslint": "3.13.0",
    "tslint-loader": "2.1.5",
    "typescript": "2.0.0",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  }
}
